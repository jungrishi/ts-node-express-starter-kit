{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {
        "max-line-length": {
            "options": [120]
        },
        "object-literal-sort-keys": false
    },
    "rules": {
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "max-line-length": {
            "options": [120]
        },
        "trailing-comma": [
            true,
            {
              "multiline": "never",
              "singleline": "never"
            }
        ],
        "semicolon":true,
        "arrow-parens": [true],
        "quotemark": [true, "single"],
        "no-console": {
            "severity": "warning",
            "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
        }
    },
    "rulesDirectory": []
}
